+            ((integer? value)
+             (format "MAKE_LITERAL_2 ~a, T_INTEGER, ~a\n" label value))
+            ((rational? value)
+             (format "MAKE_LITERAL_FRACTION ~a, ~a, ~a\n" label (numerator value) (denominator value)))
+            ((char? value)
+             (format "MAKE_LITERAL_2 ~a, T_CHAR, ~a\n" label (char->integer value)))
