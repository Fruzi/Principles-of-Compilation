+(define compile
+  (lambda (pes)
+    (let ((constants (constants-set pes))
+          (fvars (fvars-set pes)))
+      (build-consts-table constants)
+      (build-globals-table fvars))
+    (let ((code
+           (map (lambda (pe)
+                  (string-append
+                   (code-gen pe)
+                   "push rax\n"
+                   "call write_sob_if_not_void\n"
+                   "add rsp, 1*8\n"))
+                pes)))
+      (asm-epilogue (asm-prologue code)))))
